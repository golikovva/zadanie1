#include <stdio.h>
#include <stdlib.h>

int main()
{
    int *a, *b, n, i, j, t;
    scanf("%d", &n);
    a = (int*)malloc(n * sizeof(int));
    b = (int*)malloc(n * sizeof(int));
    for(i = 0; i < n; i++)
    {
        scanf("%d %d", &a[i], &b[i]);
    }
    for(i = 0; i < n - 1; i++)
    {
        for(j = 0; j < n - i - 1; j++)
        {
            if (a[j] > a[j + 1])
            {
                t = a[j];
                a[j] = a[j + 1];
                a[j + 1] = t;
            }
            if (b[j] > b[j + 1])
            {
                t = b[j];
                b[j] = b[j + 1];
                b[j + 1] = t;
            }
        }
    }
    int m = 0, h = 0;
    for (i = 0; i <n; i++)
    {
        if (b[i] >= m)
        {
            printf("%d %d\n", a[i], b[i]);
            m += a[i];
            h++;
        }
    }
    return 0;
}
